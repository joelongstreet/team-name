font_path( filename )
  '/fonts/'+filename


// embed_font loads your fonts faster - no muss, no fuss
embed_font( family_name, filename ) 
  @font-face
    font-family: family_name
    font-weight: normal
    font-style: normal
    src: url( font_path(filename+'.eot') )
    src: local('â˜º'), url( font_path(filename+'.woff')) format('woff'), url(font_path(filename+'.ttf')) format('truetype'), url(font_path(filename+'.svg#'+family_name )) format('svg')

// load fonts into font-namespaces
embed_font( 'SilkscreenNormal', 'slkscre-webfont' )
